.block {
    width: 200rpx;
    height: 200rpx;
    line-height: 200rpx;
    text-align: center;
    border-radius: 20rpx;
    background-color: #0a1d66;
    color:#fff;
}

.tags-view{
    display: flex;
    flex-direction: row;
    align-items: center;
    flex-wrap: wrap;
}
.tag-item{
    margin-left: 20rpx;
    margin-bottom: 10rpx;
}
.select{
    background-color: #0a1d66 !important;
    color: #fff !important;
}