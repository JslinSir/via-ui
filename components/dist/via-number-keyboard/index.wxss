.keyboard-cotent{position:fixed;bottom:0;left:0;right:0;display:flex;flex-direction:column;z-index:999999;transform:translateY(100%)}.keyboard-cotent .keyboard-title{height:68rpx;display:flex;flex-direction:row;align-items:center;justify-content:space-between;box-sizing:border-box;padding:0 24rpx;font-size:26rpx;color:#999;border-top:1rpx solid #f5f5f5;transform:scale(1);background-color:#fff}.keyboard-cotent-confirm{display:flex;flex-direction:row}.keyboard-panel{display:flex;flex-direction:row;flex-wrap:wrap;flex:1 1;background-color:#fff}.keyboard-item{display:flex;flex-direction:row;align-items:center;justify-content:center;height:100rpx;font-size:40rpx;font-weight:bold;flex:1 1 33.33%;box-sizing:border-box;border:1rpx solid #f5f5f5;border-bottom:none;border-left:none;transform:scale(1)}.keyboard-sign{background-color:#ededed}.clear-icon{width:48rpx;height:48rpx}.keyboard-midle-sign{flex-basis:66.67%}.right-option-view{display:flex;flex-direction:column;align-items:center;justify-content:center;min-width:180rpx;max-width:180rpx;width:180rpx;height:100%;border-top:1rpx solid #f5f5f5;transform:scale(1)}.right-clear-view{display:flex;flex-direction:row;align-items:center;justify-content:center;width:100%;height:100rpx;background-color:#fff}.confirm-button{width:180rpx !important;height:300rpx !important;border-radius:0 !important}.configdisabled{color:#fff !important;border:none !important;opacity:.6 !important}