.section-list{position:relative}.section-list .section-item-title{font-size:28rpx;color:#999;padding:10rpx 24rpx}.section-list .section-item-cell-box{display:flex;flex-direction:column;background-color:#fff}.section-list .section-item-cell{display:flex;flex-direction:row;align-items:center;height:100rpx;margin-left:24rpx;border-bottom:1px solid #f2f2f2}.section-list .section-item-cell :last-child{border-bottom:none}.section-list .section-cate{display:flex;flex-direction:column;align-items:center;justify-content:center;position:fixed;height:100%;right:0;top:0}.section-list .section-cate-text{font-size:20rpx;color:#323232;width:80rpx;height:40rpx;line-height:40rpx;text-align:right;padding-right:24rpx;position:relative}.section-list .section-cate-text .section-air{display:flex;flex-direction:row;align-items:center;justify-content:center;position:absolute;top:-11rpx;right:110rpx;width:120rpx;height:120rpx;border-radius:60rpx 60rpx 0;background:rgba(0,0,0,.6);transform:rotate(-45deg) translateY(-35%);opacity:1;transition:all .4s ease-in-out}.section-list .section-cate-text .section-air-text{color:#fff;font-size:28rpx;transform:rotate(45deg) translateY(0)}.section-list .section-air-show{opacity:1;transition:all .4s ease-in-out}.section-list .section-air-hidden{opacity:0;transition:all .4s ease-in-out}