.cotent-picker{position:relative;display:flex;height:750rpx;width:100vw;display:flex;flex-direction:column;border-radius:20rpx 20rpx;background-color:#fff}.cotent-picker .header{display:flex;flex-direction:row;align-items:center;justify-content:space-between;margin-top:60rpx}.cotent-picker .header .range-item{display:flex;flex-direction:column;align-items:center;text-align:center;justify-content:center;width:50%}.cotent-picker .header .space-line{width:60rpx;height:2rpx;background-color:#ccc}.cotent-picker .header .title{font-size:30rpx;color:#999}.cotent-picker .header .curent{display:flex;flex-direction:column;align-content:center;justify-content:center;font-size:36rpx;color:var(--theme-color, #0a1d66);margin-top:10rpx}.cotent-picker .header .plasholder{font-size:30rpx;color:#999}.cotent-picker .header .line{margin-top:8rpx;width:60rpx;height:6rpx;border-radius:6rpx;background-color:var(--theme-color, #0a1d66)}.cotent-picker .date-picker-view{display:flex;margin-top:20rpx;height:480rpx}