.via-loading {
  height: 40rpx;
  width: 40rpx;
  border-radius: 50%;
  border: 6rpx solid #cccccc;
  animation: rotate 0.6s linear infinite;
}
@keyframes rotate {
  0% {
    transform: rotate(0);
    transform-origin: center;
  }
  100% {
    transform: rotate(360deg);
    transform-origin: center;
  }
}