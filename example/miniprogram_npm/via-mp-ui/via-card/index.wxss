 

 .container {
  margin-bottom: 20rpx;
  background: #fff;
  padding: 0 20rpx;
  overflow: hidden;
}

.container.circle {
  border-radius: 20rpx;
}

.header {
  padding-top: 20rpx;
  display: flex;
  align-items: center;
}

.header.border {
  border-bottom: 1rpx solid #f2f2f2;
  padding-bottom: 20rpx;
}

.header-title {
  flex: 1;
}
