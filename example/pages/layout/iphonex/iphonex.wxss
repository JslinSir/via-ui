.fixed-bottom {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100vw;

}

.select-view {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.select-item {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    flex: 1;
    font: 30rpx;
    height: 88rpx;
    background-color: #fff;
    color: #ddd;
}

.bottom-center {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.active {

    color: #0a1d66;

}