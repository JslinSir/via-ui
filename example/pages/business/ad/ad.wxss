

.ad{
    width: 600rpx;
    height: 800rpx;
}

.ad-full-view{
    position: fixed;
    top: 0;
    bottom:0;
    right: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    background-color: #0a1d66;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.adfull{
    width: 600rpx;
    height: 600rpx;
    /* padding: 30rpx; */
    border-radius: 50%;
}
.logo-view{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    position: fixed;
    bottom:0;
    width: 100vw;
    height: 300rpx;
    background-color: #fff;
}
.logo-text{
    color: #0a1d66;
    font-size: 36rpx;
}